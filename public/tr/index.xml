    <item>
      <title>About Us</title>
      <link>http://localhost:1313/hakkimizda/</link>
      <pubDate>Mon, 01 Jan 0001 00:00:00 +0000</pubDate>
      <guid>http://localhost:1313/hakkimizda/</guid>
      <description>&lt;!DOCTYPE html&gt;&#xA;&lt;html lang=&#34;en&#34;&gt;&#xA;&#xA;&lt;head&gt;&#xA;    &lt;meta charset=&#34;UTF-8&#34;&gt;&#xA;    &lt;meta name=&#34;viewport&#34; content=&#34;width=device-width, initial-scale=1.0&#34;&gt;&#xA;    &lt;!-- Load Inter font --&gt;&#xA;    &lt;link rel=&#34;preconnect&#34; href=&#34;https://fonts.googleapis.com&#34;&gt;&#xA;    &lt;link rel=&#34;preconnect&#34; href=&#34;https://fonts.gstatic.com&#34; crossorigin&gt;&#xA;    &lt;link href=&#34;https://fonts.googleapis.com/css2?family=Inter:wght@400;500;600;700&amp;display=swap&#34; rel=&#34;stylesheet&#34;&gt;&#xA;    &lt;style&gt;&#xA;        /* Apply Inter font */&#xA;        body {&#xA;            font-family: &#39;Inter&#39;, sans-serif;&#xA;            background-color: #f9fafb;&#xA;            /* was: bg-gray-50 */&#xA;            color: #1f2937;&#xA;            /* was: text-gray-800 */&#xA;            margin: 0;&#xA;        }&#xA;&#xA;        /* Container Utility */&#xA;        .container {&#xA;            width: 150%;&#xA;            margin-left: auto;&#xA;            margin-right: auto;&#xA;            padding-left: 1.5rem;&#xA;            /* was: px-6 */&#xA;            padding-right: 1.5rem;&#xA;            /* was: px-6 */&#xA;        }&#xA;&#xA;        @media (min-width: 640px) {&#xA;            .container {&#xA;                max-width: 640px;&#xA;            }&#xA;        }&#xA;&#xA;        @media (min-width: 768px) {&#xA;            .container {&#xA;                max-width: 768px;&#xA;            }&#xA;        }&#xA;&#xA;        @media (min-width: 1024px) {&#xA;            .container {&#xA;                max-width: 1024px;&#xA;            }&#xA;        }&#xA;&#xA;        @media (min-width: 1280px) {&#xA;            .container {&#xA;                max-width: 1280px;&#xA;            }&#xA;        }&#xA;&#xA;        @media (min-width: 1536px) {&#xA;            .container {&#xA;                max-width: 1536px;&#xA;            }&#xA;        }&#xA;&#xA;        /* Header */&#xA;        .header {&#xA;            background-color: #ffffff;&#xA;            /* was: bg-white */&#xA;            box-shadow: 0 4px 6px -1px rgba(0, 0, 0, 0.1), 0 2px 4px -1px rgba(0, 0, 0, 0.06);&#xA;            /* was: shadow-md */&#xA;        }&#xA;&#xA;        .header-nav {&#xA;            padding-top: 1rem;&#xA;            /* was: py-4 */&#xA;            padding-bottom: 1rem;&#xA;            /* was: py-4 */&#xA;        }&#xA;&#xA;        .header-title {&#xA;            font-size: 1.875rem;&#xA;            /* was: text-3xl */&#xA;            line-height: 2.25rem;&#xA;            font-weight: 700;&#xA;            /* was: font-bold */&#xA;            color: #111827;&#xA;            /* was: text-gray-900 */&#xA;        }&#xA;&#xA;        /* Main Content */&#xA;        .main-content {&#xA;            padding-top: 3rem;&#xA;            /* was: py-12 */&#xA;            padding-bottom: 3rem;&#xA;            /* was: py-12 */&#xA;        }&#xA;&#xA;        /* About Section */&#xA;        .about-section {&#xA;            text-align: center;&#xA;            /* was: text-center */&#xA;            margin-bottom: 4rem;&#xA;            /* was: mb-16 */&#xA;        }&#xA;&#xA;        .about-section h1 {&#xA;            font-size: 2.25rem;&#xA;            /* was: text-4xl */&#xA;            line-height: 2.5rem;&#xA;            font-weight: 700;&#xA;            /* was: font-bold */&#xA;            color: #111827;&#xA;            /* was: text-gray-900 */&#xA;            margin-bottom: 1rem;&#xA;            /* was: mb-4 */&#xA;            margin-top: 0;&#xA;        }&#xA;&#xA;        .about-section p {&#xA;            font-size: 1.125rem;&#xA;            /* was: text-lg */&#xA;            line-height: 1.75rem;&#xA;            color: #374151;&#xA;            /* was: text-gray-700 */&#xA;            max-width: 48rem;&#xA;            /* was: max-w-3xl */&#xA;            margin-left: auto;&#xA;            /* was: mx-auto */&#xA;            margin-right: auto;&#xA;            /* was: mx-auto */&#xA;        }&#xA;&#xA;        /* Team Section */&#xA;        .team-section h2 {&#xA;            font-size: 1.875rem;&#xA;            /* was: text-3xl */&#xA;            line-height: 2.25rem;&#xA;            font-weight: 700;&#xA;            /* was: font-bold */&#xA;            text-align: center;&#xA;            /* was: text-center */&#xA;            color: #111827;&#xA;            /* was: text-gray-900 */&#xA;            margin-bottom: 3rem;&#xA;            /* was: mb-12 */&#xA;            margin-top: 0;&#xA;        }&#xA;&#xA;        .team-grid {&#xA;            display: grid;&#xA;            grid-template-columns: repeat(1, minmax(0, 1fr));&#xA;            /* was: grid-cols-1 */&#xA;            gap: 2.5rem;&#xA;            /* was: gap-10 */&#xA;        }&#xA;&#xA;        @media (min-width: 768px) {&#xA;&#xA;            /* was: md: */&#xA;            .team-grid {&#xA;                grid-template-columns: repeat(2, minmax(0, 1fr));&#xA;                /* was: md:grid-cols-2 */&#xA;            }&#xA;        }&#xA;&#xA;        @media (min-width: 1024px) {&#xA;&#xA;            /* was: lg: */&#xA;            .team-grid {&#xA;                grid-template-columns: repeat(3, minmax(0, 1fr));&#xA;                /* was: lg:grid-cols-3 */&#xA;            }&#xA;        }&#xA;&#xA;        /* Team Card */&#xA;        .team-card {&#xA;            background-color: #ffffff;&#xA;            /* was: bg-white */&#xA;            border-radius: 0.5rem;&#xA;            /* was: rounded-lg */&#xA;            box-shadow: 0 10px 15px -3px rgba(0, 0, 0, 0.1), 0 4px 6px -2px rgba(0, 0, 0, 0.05);&#xA;            /* was: shadow-lg */&#xA;            overflow: hidden;&#xA;            /* was: overflow-hidden */&#xA;            text-align: center;&#xA;            /* was: text-center */&#xA;            transition: transform 0.3s ease-in-out;&#xA;            /* was: transition-transform */&#xA;        }&#xA;&#xA;        .team-card:hover {&#xA;            transform: scale(1.05);&#xA;            /* was: hover:scale-105 */&#xA;        }&#xA;&#xA;        .team-card img {&#xA;            width: 100%;&#xA;            /* was: w-full */&#xA;            height: 16rem;&#xA;            /* was: h-64 */&#xA;            object-fit: cover;&#xA;            /* was: object-cover */&#xA;            display: block;&#xA;        }&#xA;&#xA;        .team-card-content {&#xA;            padding: 1.5rem;&#xA;            /* was: p-6 */&#xA;        }&#xA;&#xA;        .team-card-content h3 {&#xA;            font-size: 1.5rem;&#xA;            /* was: text-2xl */&#xA;            line-height: 2rem;&#xA;            font-weight: 600;&#xA;            /* was: font-semibold */&#xA;            color: #111827;&#xA;            /* was: text-gray-900 */&#xA;            margin-bottom: 0.25rem;&#xA;            /* was: mb-1 */&#xA;            margin-top: 0;&#xA;        }&#xA;&#xA;        .team-card-content .title {&#xA;            font-size: 1rem;&#xA;            line-height: 1.5rem;&#xA;            font-weight: 500;&#xA;            /* was: font-medium */&#xA;            margin-bottom: 1rem;&#xA;            /* was: mb-4 */&#xA;            margin-top: 0;&#xA;        }&#xA;&#xA;        .title-1 {&#xA;            color: #65a30d;&#xA;        }&#xA;&#xA;        /* was: text-lime-700 */&#xA;        .title-2 {&#xA;            color: #1d4ed8;&#xA;        }&#xA;&#xA;        /* was: text-blue-700 */&#xA;        .title-3 {&#xA;            color: #b91c1c;&#xA;        }&#xA;&#xA;        /* was: text-red-700 */&#xA;&#xA;        .team-card-content .bio {&#xA;            color: #4b5563;&#xA;            /* was: text-gray-600 */&#xA;            margin: 0;&#xA;        }&#xA;&#xA;        /* Footer */&#xA;        .footer {&#xA;            background-color: #1f2937;&#xA;            /* was: bg-gray-800 */&#xA;            color: #ffffff;&#xA;            /* was: text-white */&#xA;            margin-top: 4rem;&#xA;            /* was: mt-16 */&#xA;        }&#xA;&#xA;        .footer-content {&#xA;            padding-top: 2rem;&#xA;            /* was: py-8 */&#xA;            padding-bottom: 2rem;&#xA;            /* was: py-8 */&#xA;            text-align: center;&#xA;            /* was: text-center */&#xA;        }&#xA;&#xA;        .footer-content p {&#xA;            margin: 0;&#xA;        }&#xA;    &lt;/style&gt;&#xA;&lt;/head&gt;&#xA;&#xA;&lt;body&gt;&#xA;    &lt;!-- Main Content --&gt;&#xA;    &lt;main class=&#34;container main-content&#34;&gt;&#xA;&#xA;        &lt;!-- About Us Section --&gt;&#xA;        &lt;section class=&#34;about-section&#34;&gt;&#xA;            &lt;h1&gt;About Us&lt;/h1&gt;&#xA;            &lt;p&gt;&#xA;                Welcome to Fethullah Gulen Legacy Foundation! We are a passionate team dedicated to preserve, promote,&#xA;                and protect the values of Fethullah Gulen. Our goal is to create institutions where transparency and&#xA;                accountability exceed standards of our society.&#xA;            &lt;/p&gt;</description>
